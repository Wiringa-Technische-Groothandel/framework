{
  "name": "wtg/framework",
  "description": "WTG Webshop framework.",
  "keywords": ["framework", "wtg", "laravel", "webshop"],
  "license": "MIT",
  "type": "library",
  "homepage": "https://wiringa.nl",
  "authors": [
    {
      "name": "Thomas Wiringa",
      "email": "thomas.wiringa@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "laravel-doctrine/acl": "^1.0",
    "laravel-doctrine/orm": "^1.4",
    "laravel/framework": "5.5.*",
    "ramsey/uuid-doctrine": "^1.4"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "files": [
      "src/WTG/ContentManager/helpers.php"
    ],
    "psr-4": {
      "WTG\\": "src/WTG"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WTG\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
